{
  "name": "mifare-reader-js",
  "version": "1.0.0",
  "description": "A TypeScript library for interfacing with MIFARE card readers via serial port communication",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/create-esm-package.js",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mifare",
    "rfid",
    "card-reader",
    "serialport",
    "nfc",
    "mifare-classic",
    "rfid-reader",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "serialport": "^13.0.0"
  }
}
